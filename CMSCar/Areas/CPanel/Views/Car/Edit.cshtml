@model CarDTO
@{
    ViewData["Title"] = "تعديل سيارة  ";
}
@section Css{
    <style>
        td {
            padding: 8px 1px 8px 1px !important;
            font-size: 14px !important;
        }

        th {
            font-weight: bold;
            font-size: medium;
        }

        td a {
            margin-left: 5px !important;
        }

        .m-portlet__body {
            padding-top: 0px !important;
        }

        .dt-buttons {
            margin-top: 12px
        }
    </style>
}
<div class="m-portlet m-portlet--mobile">
    <div class="m-portlet__head">
        <div class="m-portlet__head-caption">
            <div class="m-portlet__head-title">
                <h3 class="m-portlet__head-text">
                    <span style="color: #ebebeb"></span> تعديل بيانات @Model.NameAr
                </h3>
            </div>
        </div>
    </div>
    <div class="m-portlet__body">
        <br />
        <form asp-action="Edit" method="post" enctype="multipart/form-data">
            <input type="hidden" asp-for="Id" />
            <div class="row">
                <div class="col-md-6">
                    <div class="form-group">
                        <label asp-for="NameAr" class="control-label"></label>
                        <input asp-for="NameAr" class="form-control">
                        <span asp-validation-for="NameAr" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="form-group">
                        <label asp-for="NameEn" class="control-label"></label>
                        <input asp-for="NameEn" class="form-control">
                        <span asp-validation-for="NameEn" class="text-danger"></span>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <div class="form-group">
                        <label asp-for="PriceBeforeDiscount" class="control-label"></label>
                        <input type="number" asp-for="PriceBeforeDiscount" class="form-control">
                        <span asp-validation-for="PriceBeforeDiscount" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="form-group">
                        <label asp-for="PriceAfterDiscount" class="control-label"></label>
                        <input type="number" asp-for="PriceAfterDiscount" class="form-control">
                        <span asp-validation-for="PriceAfterDiscount" class="text-danger"></span>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-6 col-md-4">
                    <div class="form-group">
                        <label class="text-primary control-label"></label>
                        <select id="CarType" data-size="6" data-live-search="true" class="rounded selectpicker" data-width="100%">
                            <option value="">--- إختار  النوع الرئيسي ---</option>
                        </select>
                        <span class="text-danger"></span>
                    </div>
                </div>
                <div class="col-6 col-md-4">
                    <div class="form-group">
                        <label class="text-primary control-label"></label>
                        <select asp-for="SubCarTypeId" id="SubCarTypeSelect" data-size="6" data-live-search="true" class="rounded selectpicker" data-width="100%">
                            <option value="">--- إختار  النوع الفرعي ---</option>
                        </select>
                        <span asp-validation-for="SubCarTypeId" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-6 col-md-4">
                    <div class="form-group">
                        <label class="text-primary control-label"></label>
                        <select asp-for="ModelCarTypeId" id="ModelCarTypeSelect" data-size="6" data-live-search="true" class="rounded selectpicker" data-width="100%">
                            <option value="">--- إختار الموديل ---</option>
                        </select>
                        <span asp-validation-for="ModelCarTypeId" class="text-danger"></span>
                    </div>
                </div>

            </div>
            <div class="row">
                <div class="col-md-4">
                    <div class="form-group">
                        <label asp-for="Main" class="control-label"></label>
                        <input type="file" asp-for="Main" class="form-control">
                        <span asp-validation-for="Main" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group">
                        <label asp-for="Show" class="control-label"></label>
                        <input type="file" asp-for="Show" class="form-control">
                        <span asp-validation-for="Show" class="text-danger"></span>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group">
                        <label asp-for="InsidImages" class="control-label"></label>
                        <input type="file" asp-for="InsidImages" multiple class="form-control">
                        <span asp-validation-for="InsidImages" class="text-danger"></span>
                    </div>
                </div>

            </div>
            <div class="m-form m-form--label-align-right m--margin-bottom-0">
                <div class="row align-items-center">
                    <div class="col-xl-12 order-2 order-xl-1">
                        <div class="row">
                            <div class="col-md-4">
                                <a class="btn btn-success" style="font-size:medium" data-toggle="collapse" href="#collapseExample" role="button" aria-expanded="false" aria-controls="collapseExample">
                                    إضافات السيارة
                                </a>
                            </div>
                        </div>
                        <div class="collapse" id="collapseExample">
                            <br />
                            <div class="p-3 mb-2 text-white" style="background-color:crimson">
                                ميزات السيارة
                            </div>
                            <br />
                            <div class="m-form m-form--label-align-right m--margin-bottom-0">
                                <div class="row align-items-center">
                                    <div class="col-xl-12 order-2 order-xl-1">
                                        <div class="row">
                                            @if (Model.featureCars.Any())
                                            {
                                                foreach (var item in Model.featureCars)
                                                {
                                                    if (item.isTake)
                                                    {
                                                        <div class="col-md-4">
                                                            <div class="card-header">
                                                                <input name="feature" checked value="@item.Id" type="checkbox">
                                                                <a style="color: black;text-decoration: none;" data-toggle="collapse" href="#collapseExample_@item.Id" role="button" aria-expanded="false" aria-controls="collapseExample">
                                                                    <b>@item.NameAr</b>
                                                                </a>
                                                            </div>
                                                            <div class="collapse" id="collapseExample_@item.Id">
                                                                <div class="m-form__group form-group">
                                                                    <div class="m-checkbox-list">
                                                                        <ul class="list-group list-group-flush">
                                                                            @foreach (var item1 in item.subFeatureCars)
                                                                            {
                                                                                <li class="list-group-item">

                                                                                    @if (!string.IsNullOrEmpty(item1.AnswerAr) || !string.IsNullOrEmpty(item1.AnswerAr))
                                                                                    {
                                                                                        <label class="custom-checkbox">
                                                                                            <input name="subFeatureCars[]" checked onchange="change('checkbox_@item1.Id', 'text_@item1.Id', 'entext_@item1.Id');" id="checkbox_@item1.Id" value="@item1.Id" type="checkbox">
                                                                                            @item1.NameAr
                                                                                        </label>
                                                                                        <div class="row">
                                                                                            <div class="col-md-6">
                                                                                                <input name="arValues[]" placeholder="القيمة بالعربية" id="text_@item1.Id" class="form-control" value="@item1.AnswerAr" type="text">
                                                                                            </div>
                                                                                            <div class="col-md-6">
                                                                                                <input name="enValues[]" placeholder="القيمة بالانجليزية" id="entext_@item1.Id" class="form-control" value="@item1.AnswerAr" type="text">
                                                                                            </div>
                                                                                        </div>

                                                                                    }
                                                                                    else
                                                                                    {
                                                                                        <label class="custom-checkbox">
                                                                                            <input name="subFeatureCars[]" onchange="change('checkbox_@item1.Id', 'text_@item1.Id', 'entext_@item1.Id');" id="checkbox_@item1.Id" value="@item1.Id" type="checkbox">
                                                                                            @item1.NameAr
                                                                                        </label>
                                                                                        <div class="row">
                                                                                            <div class="col-md-6">
                                                                                                <input name="arValues[]" disabled placeholder="القيمة بالعربية" id="text_@item1.Id" class="form-control" value="@item1.AnswerAr" type="text">
                                                                                            </div>
                                                                                            <div class="col-md-6">
                                                                                                <input name="enValues[]" disabled placeholder="القيمة بالانجليزية" id="entext_@item1.Id" class="form-control" value="@item1.AnswerAr" type="text">
                                                                                            </div>
                                                                                        </div>

                                                                                    }

                                                                                </li>
                                                                            }
                                                                        </ul>
                                                                        <br />
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>

                                                    }
                                                    else
                                                    {
                                                        <div class="col-md-4">
                                                            <div class="card-header">
                                                                <input name="feature" value="@item.Id" type="checkbox">
                                                                <a style="color: black;text-decoration: none;" data-toggle="collapse" href="#collapseExample_@item.Id" role="button" aria-expanded="false" aria-controls="collapseExample">
                                                                    <b>@item.NameAr</b>
                                                                </a>
                                                            </div>
                                                            <div class="collapse" id="collapseExample_@item.Id">
                                                                <div class="m-form__group form-group">
                                                                    <div class="m-checkbox-list">
                                                                        <ul class="list-group list-group-flush">
                                                                            @foreach (var item1 in item.subFeatureCars)
                                                                            {
                                                                                <li class="list-group-item">
                                                                                    <label class="custom-checkbox">
                                                                                        <input name="subFeatureCars[]" onchange="change('checkbox_@item1.Id', 'text_@item1.Id', 'entext_@item1.Id');" id="checkbox_@item1.Id" value="@item1.Id" type="checkbox">
                                                                                        @item1.NameAr
                                                                                    </label>
                                                                                    <div class="row">
                                                                                        <div class="col-md-6">
                                                                                            <input name="arValues[]" disabled placeholder="القيمة بالعربية" id="text_@item1.Id" class="form-control" value="" type="text">
                                                                                        </div>
                                                                                        <div class="col-md-6">
                                                                                            <input name="enValues[]" disabled placeholder="القيمة بالانجليزية" id="entext_@item1.Id" class="form-control" value="" type="text">
                                                                                        </div>
                                                                                    </div>

                                                                                </li>
                                                                            }
                                                                        </ul>
                                                                        <br />
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>

                                                    }
                                                }
                                            }
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="p-3 mb-2 text-white" style="background-color:crimson">
                                مواصفات السيارة
                            </div>
                            <div class="m-form m-form--label-align-right m--margin-bottom-0">
                                <div class="row align-items-center">
                                    <div class="col-xl-12 order-2 order-xl-1">
                                        <div class="row">
                                            @if (Model.specificationCars.Any())
                                            {
                                                foreach (var item in Model.specificationCars)
                                                {
                                                    if (item.isTake)
                                                    {<div class="col-md-4">
                                                            <div class="card-header">
                                                                <input name="SepCars" checked value="@item.Id" type="checkbox">
                                                                <a style="color: black;text-decoration: none;" data-toggle="collapse" href="#collapseExamples_@item.Id" role="button" aria-expanded="false" aria-controls="collapseExample">
                                                                    <b>@item.NameAr</b>
                                                                </a>
                                                            </div>
                                                            <div class="collapse" id="collapseExamples_@item.Id">
                                                                <div class="m-form__group form-group">
                                                                    <div class="m-checkbox-list">
                                                                        <ul class="list-group list-group-flush">
                                                                            @foreach (var item1 in item.subSpecificationCars)
                                                                            {
                                                                                <li class="list-group-item">
                                                                                    @if (!string.IsNullOrEmpty(item1.AnswerAr) || !string.IsNullOrEmpty(item1.AnswerAr))
                                                                                    {
                                                                                        <label class="custom-checkbox">
                                                                                            <input name="subSepCars[]" checked onchange="change('checkbox_Sep_@item1.Id', 'text_Sep_@item1.Id', 'entext_Sep_@item1.Id');" id="checkbox_Sep_@item1.Id" value="@item1.Id" type="checkbox">
                                                                                            @item1.NameAr
                                                                                        </label>
                                                                                        <div class="row">
                                                                                            <div class="col-md-6">
                                                                                                <input name="SepAr[]" placeholder="القيمة بالعربية" id="text_Sep_@item1.Id" class="form-control" value="@item1.AnswerAr" type="text">
                                                                                            </div>
                                                                                            <div class="col-md-6">
                                                                                                <input name="SepEn[]" placeholder="القيمة بالانجليزية" id="entext_Sep_@item1.Id" class="form-control" value="@item1.AnswerEn" type="text">
                                                                                            </div>
                                                                                        </div>
                                                                                    }
                                                                                    else
                                                                                    {
                                                                                        <label class="custom-checkbox">
                                                                                            <input name="subSepCars[]" onchange="change('checkbox_Sep_@item1.Id', 'text_Sep_@item1.Id', 'entext_Sep_@item1.Id');" id="checkbox_Sep_@item1.Id" value="@item1.Id" type="checkbox">
                                                                                            @item1.NameAr
                                                                                        </label>
                                                                                        <div class="row">
                                                                                            <div class="col-md-6">
                                                                                                <input name="SepAr[]" disabled placeholder="القيمة بالعربية" id="text_Sep_@item1.Id" class="form-control" value="@item1.AnswerAr" type="text">
                                                                                            </div>
                                                                                            <div class="col-md-6">
                                                                                                <input name="SepEn[]" disabled placeholder="القيمة بالانجليزية" id="entext_Sep_@item1.Id" class="form-control" value="@item1.AnswerEn" type="text">
                                                                                            </div>
                                                                                        </div>
                                                                                    }
                                                                                </li>
                                                                            }
                                                                        </ul>
                                                                        <br />
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>

                                                    }
                                                    else
                                                    {
                                                        <div class="col-md-4">
                                                            <div class="card-header">
                                                                <input name="SepCars" value="@item.Id" type="checkbox">
                                                                <a style="color: black;text-decoration: none;" data-toggle="collapse" href="#collapseExamples_@item.Id" role="button" aria-expanded="false" aria-controls="collapseExample">
                                                                    <b>@item.NameAr</b>
                                                                </a>
                                                            </div>
                                                            <div class="collapse" id="collapseExamples_@item.Id">
                                                                <div class="m-form__group form-group">
                                                                    <div class="m-checkbox-list">
                                                                        <ul class="list-group list-group-flush">
                                                                            @foreach (var item1 in item.subSpecificationCars)
                                                                            {
                                                                                <li class="list-group-item">
                                                                                    <label class="custom-checkbox">
                                                                                        <input name="subSepCars[]" onchange="change('checkbox_Sep_@item1.Id', 'text_Sep_@item1.Id', 'entext_Sep_@item1.Id');" id="checkbox_Sep_@item1.Id" value="@item1.Id" type="checkbox">
                                                                                        @item1.NameAr
                                                                                    </label>
                                                                                    <div class="row">
                                                                                        <div class="col-md-6">
                                                                                            <input name="SepAr[]" disabled placeholder="القيمة بالعربية" id="text_Sep_@item1.Id" class="form-control" type="text">
                                                                                        </div>
                                                                                        <div class="col-md-6">
                                                                                            <input name="SepEn[]" disabled placeholder="القيمة بالانجليزية" id="entext_Sep_@item1.Id" class="form-control" type="text">
                                                                                        </div>
                                                                                    </div>
                                                                                </li>
                                                                            }
                                                                        </ul>
                                                                        <br />
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>

                                                    }
                                                }
                                            }
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <br />
                            <div class="m-portlet m-portlet--mobile">
                                <div class="m-portlet__head">
                                    <div class="m-portlet__head-caption">
                                        <div class="m-portlet__head-title">
                                            <h3 class="m-portlet__head-text">
                                                <span style="color: #ebebeb"></span> اضافة الالوان
                                            </h3>
                                        </div>
                                    </div>
                                    <div class="m-portlet__head-tools">
                                        <ul class="m-portlet__nav">
                                            <li class="m-portlet__nav-item">
                                                <a href="~/CPanel/CarExtinsion/AddColorEdit/@Model.Id" title="اضافة لون جديد" class="PopUp m-btn--sm btn btn-success m-btn m-btn--custom m-btn--icon m-btn--pill">
                                                    <span>
                                                        <i class="fa fa-plus-square"></i>
                                                        <span style="font-size:14px">اضافة لون جديد</span>
                                                    </span>
                                                </a>
                                            </li>


                                        </ul>
                                    </div>
                                </div>
                                <div class="m-portlet__body">
                                    <table class="table m-table table-bordered table-striped m-table--head-separator-success" id="color">
                                        <thead align="center">
                                            <tr>
                                                <th>اسم اللون بالعربية</th>
                                                <th>اسم اللون بالانجليزية</th>
                                                <th>تاريخ الاضافة</th>
                                                <th width="20%">العمليات</th>
                                            </tr>
                                        </thead>
                                        <tbody align="center"></tbody>
                                    </table>

                                </div>

                            </div>

                        </div>
                    </div>
                </div>
            </div>
            <br />
            <div class="row">
                <div class="col-md-6">
                    <div class="p-3 mb-2 text-white" style="background-color:crimson">
                        الوصف المصغر بالعربية
                    </div>
                    <span asp-validation-for="SubDescriptionAr" class="text-danger"></span>
                    <textarea class="summernote" asp-for="SubDescriptionAr"></textarea>
                </div>
                <div class="col-md-6">
                    <div class="p-3 mb-2 text-white" style="background-color:crimson">
                        الوصف المصغر  بالإنجليزية
                    </div>
                    <span asp-validation-for="SubDescriptionEn" class="text-danger"></span>
                    <textarea class="summernoteEN" asp-for="SubDescriptionEn"></textarea>
                </div>
            </div>
            <hr />
            <br />
            <div class="row">
                <div class="col-md-6">
                    <div class="p-3 mb-2 text-white" style="background-color:crimson">
                        وصف السيارة بالعربية
                    </div>
                    <span asp-validation-for="DescriptionAr" class="text-danger"></span>
                    <textarea class="summernote" asp-for="DescriptionAr"></textarea>
                </div>
                <div class="col-md-6">
                    <div class="p-3 mb-2 text-white" style="background-color:crimson">
                        وصف السيارة بالإنجليزية
                    </div>
                    <span asp-validation-for="DescriptionEn" class="text-danger"></span>
                    <textarea class="summernoteEN" asp-for="DescriptionEn"></textarea>
                </div>
            </div>
            <hr />
            <div class="form-group">
                <input type="submit" value="حفظ" class="btn btn-primary" />
                <button type="button" class="btn btn-default waves-effect" data-dismiss="modal">الغاء</button>
            </div>
        </form>
    </div>
</div>



@section Scripts{
    <script>
        $('.summernote').summernote({
            placeholder: 'ادخل المحتوى  ...',
            tabsize: 1,
            height: 150
        });
    </script>
    <script>
        $('.summernoteEN').summernote({
            placeholder: 'ادخل المحتوى باللغة الانجليزية  ...',
            tabsize: 1,
            height: 150
        });
    </script>
    <script>
        $('.dataLive').selectpicker();
    </script>
  
    <script>
    $.ajax({
        url: '/CPanel/Car/GetTypeCar',
            dataType: "json",
            success: function (Data) {
                $(Data).each(function (index, value) {
                    if (value.id == @Model.CarTypeId) {
                        $("#CarType").append($("<option selected></option>").val(value.id).html(value.nameAr));
                    } else {
                        $("#CarType").append($("<option></option>").val(value.id).html(value.nameAr));
                    }
                    $("#CarType").selectpicker('refresh');
                })
            },
            complete: function (Data) {
                $('#SubCarTypeSelect option[value!=""]').remove();
                $("#SubCarTypeSelect").selectpicker('refresh');
                $.ajax({
                    url: '/CPanel/Car/GetSubTypeCar/' + $('#CarType').val(),
                    dataType: "json",
                    success: function (Data) {
                        $(Data).each(function (index, value) {
                            if (value.id == @Model.SubCarTypeId) {
                                $("#SubCarTypeSelect").append($("<option selected></option>").val(value.id).html(value.nameAr));
                            } else {
                                $("#SubCarTypeSelect").append($("<option></option>").val(value.id).html(value.nameAr));
                            }
                            $("#SubCarTypeSelect").selectpicker('refresh');
                        })
                    },
                    complete: function (Data) {
                        $('#ModelCarTypeSelect option[value!=""]').remove();
                        $("#ModelCarTypeSelect").selectpicker('refresh');
                        $.ajax({
                            url: '/CPanel/Car/GetModel/' + $('#CarType').val(),
                            dataType: "json",
                            success: function (Data) {
                                $(Data).each(function (index, value) {
                                    if (value.id == @Model.ModelCarTypeId) {
                                        $("#ModelCarTypeSelect").append($("<option selected></option>").val(value.id).html(value.nameAr));
                                    } else {
                                        $("#ModelCarTypeSelect").append($("<option></option>").val(value.id).html(value.nameAr));
                                    }
                                    $("#ModelCarTypeSelect").selectpicker('refresh');
                                })
                            }
                        });
                    }
                });
            }
        });

        $('#CarType').on('changed.bs.select', function (e, clickedIndex, isSelected, previousValue) {
            $('#SubCarTypeSelect option[value!=""]').remove();
            $("#CarType").selectpicker('refresh');
            $("#SubCarTypeSelect").selectpicker('refresh');
            $('#ModelCarTypeSelect option[value!=""]').remove();
            $("#ModelCarTypeSelect").selectpicker('refresh');
            $.ajax({
                url: '/CPanel/Car/GetSubTypeCar/' + $('#CarType').val(),
                dataType: "json",
                success: function (Data) {
                    $(Data).each(function (index, value) {
                        $("#SubCarTypeSelect").append($("<option></option>").val(value.id).html(value.nameAr)).selectpicker('refresh');
                    })
                }
            });
        });

        $('#SubCarTypeSelect').on('changed.bs.select', function (e, clickedIndex, isSelected, previousValue) {
            $('#ModelCarTypeSelect option[value!=""]').remove();
            $("#ModelCarTypeSelect").selectpicker('refresh');
            $.ajax({
                url: '/CPanel/Car/GetModel/' + $('#CarType').val(),
                dataType: "json",
                success: function (Data) {
                    $(Data).each(function (index, value) {
                        $("#ModelCarTypeSelect").append($("<option></option>").val(value.id).html(value.nameAr)).selectpicker('refresh');
                    })
                }
            });
        });
    </script>




    <script>
        function change(checkId, inputId, enInputID) {
            var chek = document.getElementById(checkId);
            if ($('#' + checkId + '').is(":checked")) {
                document.getElementById(inputId).disabled = false;
                document.getElementById(enInputID).disabled = false;
            } else {
                document.getElementById(inputId).disabled = true;
                document.getElementById(enInputID).disabled = true;

            }

        };
    </script>
    <script>
        $(function () {
            $(".DTForm").submit(function () {
                BindDataTable();
                return false;
            });
            BindDataTable();
        });

        $(document).on("click", ".SearchBtn", function () {
            BindDataTable();
        });

        BindDataTable();

        function BindDataTable() {
            oTable = $('#color').dataTable(
                {
                    language: {
                        aria: {
