@model CMSCar.Areas.CPanel.DTOs.SubFS.SubFeatureCarUpdateDTO
@{
    ViewData["Title"] = "تعديل ميزة فرعية ";
    Layout = null;
}

<form asp-action="EditSF" method="post" class="ajaxForm" >
    <input asp-for="Id" type="hidden" />
    <div class="row">
        <div class="col-md-6">
            <div class="form-group">
                <label asp-for="NameAr"></label>
                <input asp-for="NameAr" class="form-control" />
                <span asp-validation-for="NameAr" class="text-danger"></span>
            </div>
        </div>
        <div class="col-md-6">
            <div class="form-group">
                <label asp-for="NameEn"></label>
                <input asp-for="NameEn" class="form-control" />
                <span asp-validation-for="NameEn" class="text-danger"></span>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-6">
            <div class="form-group">
                <label asp-for="AnswerAr"></label>
                <input asp-for="AnswerAr" class="form-control" />
                <span asp-validation-for="AnswerAr" class="text-danger"></span>
            </div>
        </div>
        <div class="col-md-6">
            <div class="form-group">
                <label asp-for="AnswerEn"></label>
                <input asp-for="AnswerEn" class="form-control" />
                <span asp-validation-for="AnswerEn" class="text-danger"></span>
            </div>
        </div>
    </div>

    <hr />
    <div class="form-group">
        <input type="submit" value="حفظ" class="btn btn-primary" />
        <button type="button" class="btn btn-default waves-effect" data-dismiss="modal">الغاء</button>
    </div>
</form>
<script>
    PageLoadActions();
</script>
<script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>