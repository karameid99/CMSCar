        // else if is Desktop Fixed purchace tab on Top
        var isMobile = window.matchMedia("only screen and (max-width: 760px)").matches;
        if (isMobile) {
            function fixDiv() {
                var $cache = $('#getFixed');
                if ($(window).scrollTop() > 300) {
                    $cache.slideDown();
                    document.getElementById('getFixed').style.display = "block";
                    $cache.css({
                        'position': 'fixed',
                        'bottom': '0',
                        'top': 'auto',
                        'display': 'block'
                    });
                    $('#footer').css({
                        'margin-bottom': '50px',
                    });
                    $('#callOnFixed').hide();
                } else {
                    $cache.slideUp();
                    document.getElementById('getFixed').style.display = "none";
                    $cache.css({
                        'position': 'relative',
                        'bottom': 'auto',
                        'display': 'none',
                    });
                    $('#footer').css({
                        'margin-bottom': '0',
                    });
                }
            }
            $(window).scroll(fixDiv);
            fixDiv();
        } else {
            function fixDiv() {
                var $cache = $('#getFixed');
                if ($(window).scrollTop() > 300) {
                    $cache.slideDown('fast');
                    $cache.css({
                        'position': 'fixed',
                        'top': '0',
                        'display': 'block'
                    });
                } else {
                    $cache.slideUp();
                    $cache.css({
                        'position': 'relative',
                        'top': 'auto',
                        'display': 'none'
                    });
                }
            }
            $(window).scroll(fixDiv);
            fixDiv();
        }
    </script>
